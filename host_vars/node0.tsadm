---
sites_all:
  - {uid: 61001, name: 'regr'}
  - {uid: 61002, name: 's0'}


sites_all_auth:
  - name: 'regr'
    auth_keys:
      - state: present
        key: 'ssh-rsa  TSADM-AT-NODE0'
      - state: 'present'
        key: 'ssh-rsa  JRMS-KEY'
      - state: 'present'
        key: 'ssh-rsa  IBONELLI-KEY'
  - name: 's0'
    auth_keys:
      - state: present
        key: 'ssh-rsa  TSADM-AT-NODE0'
      - state: present
        key: 'ssh-rsa  JRMS-KEY'
      - state: 'present'
        key: 'ssh-rsa  IBONELLI-KEY'
      - state: 'present'
        key: 'ssh-rsa  IBONELLI-KEY2'


sites_envs_all:
  - site_name: 'regr'
    uid: 61001
    envs:
      - name: 'dev'
        host_fqdn: 'node0.tsadm'
        host_www_port: 80
        git_uri: 'ssh://regr@tsadm.chroot:22/~/regr.git'
      - name: 'test'
        host_fqdn: 'node0.tsadm'
        host_www_port: 80
        git_uri: 'ssh://regr@tsadm.chroot:22/~/regr.git'
  - site_name: 's0'
    uid: 61002
    envs:
      - name: 'dev'
        host_fqdn: 'node0.tsadm'
        host_www_port: 80
        git_uri: 'ssh://s0@tsadm.chroot:22/~/s0.git'
